{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/chat/(.*)",
      "destination": "/api/proxy"
    },
    {
      "source": "/upload/(.*)",
      "destination": "/api/proxy"
    },
    {
      "source": "/loadImg/(.*)",
      "destination": "/api/proxy"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
